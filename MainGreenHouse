while True:
   event, values =window.read()
   if event == sg.WINDOW_CLOSED:
       break
   if event=='-but1-':
       window=sg.Window('',plante1_layout,size=(410,410),finalize=True)
       
       if event== 'back':
        window=sg.Window('', start_layout, size=(410,410),finalize=True)
   if event=='-but2-':
       window=sg.Window('',plante2_layout,size=(410,410),finalize=True)
   if event=='-but3-':
       window=sg.Window('',plante3_layout,size=(410,410),finalize=True)
