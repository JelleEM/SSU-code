import PySimpleGUI as sg

start_layout=[[sg.Text('Dine Planter',key='t',background_color='dark sea green')],
        [sg.Button('Plante 1', key='-but1-')], 
        [sg.Button('Plante 2', key='-but2-')],
        [sg.Button('Plante 3', key='-but3-')]]
        
plante1_layout=[[sg.Text('Plante 1',key='t',background_color='dark sea green')],
                [sg.Button('back', key='back')]]
plante2_layout=[[sg.Text('Plante 2',key='t',background_color='dark sea green')],
                [sg.Button('back', key='back')]]  
plante3_layout=[[sg.Text('Plante 3',key='t',background_color='dark sea green')],
                [sg.Button('back', key='back')]]
window=sg.Window('',start_layout,size=(410,410),finalize=True) 
app

